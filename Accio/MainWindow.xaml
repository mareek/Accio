<Window x:Class="Accio.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Accio"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <Button Name="DownloadButton" DockPanel.Dock="Top" Click="DownloadButton_Click" IsDefault="True">Get availability</Button>
        <WebBrowser Name="Browser" DockPanel.Dock="Top" Height="20"></WebBrowser>
        <TextBox Name="OutputBox" />
    </DockPanel>
</Window>
